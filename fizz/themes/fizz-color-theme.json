{
  "name": "fizz",
  "type": "dark",
  "colors": {
    // Base workbench colors
    "foreground": "#E5D0AC",
    "focusBorder": "#3674B5",
    "selection.background": "#0c2a45",
    
    // Activity Bar
    "activityBar.background": "#253543",
    "activityBar.foreground": "#FF9D23",
    "activityBar.inactiveForeground": "#E5D0AC80",
    "activityBarBadge.background": "#C14600",
    "activityBarBadge.foreground": "#FEF9E1",
    
    // Sidebar
    "sideBar.background": "#162e44",
    "sideBar.foreground": "#E5D0AC",
    "sideBarTitle.foreground": "#FF9D23",
    "sideBarSectionHeader.background": "#253543",
    "sideBarSectionHeader.foreground": "#FEF9E1",
    "sideBarSectionHeader.border": "#3D8D7A40",
    
    // Lists
    "list.activeSelectionBackground": "#3674B5",
    "list.activeSelectionForeground": "#FEF9E1",
    "list.inactiveSelectionBackground": "#0c2a45",
    "list.hoverBackground": "#3D8D7A40",
    "list.focusBackground": "#3674B5",
    "list.focusForeground": "#FEF9E1",
    "list.highlightForeground": "#FF9D23",
    
    // Editor
    "editor.background": "#0c2a45",
    "editor.foreground": "#E5D0AC",
    "editorLineNumber.foreground": "#3D8D7A",
    "editorLineNumber.activeForeground": "#FF9D23",
    "editor.selectionBackground": "#3674B5",
    "editor.selectionHighlightBackground": "#3674B580",
    "editor.wordHighlightBackground": "#3D8D7A40",
    "editor.findMatchBackground": "#C14600",
    "editor.findMatchHighlightBackground": "#FF9D2340",
    "editor.lineHighlightBackground": "#3D8D7A20",
    "editor.lineHighlightBorder": "#3D8D7A00",
    "editorCursor.foreground": "#FF9D23",
    "editorWhitespace.foreground": "#3D8D7A40",
    "editorIndentGuide.background": "#3D8D7A40",
    "editorIndentGuide.activeBackground": "#3D8D7A",
    
    // Status Bar
    "statusBar.background": "#C14600",
    "statusBar.foreground": "#FEF9E1",
    "statusBar.debuggingBackground": "#FF9D23",
    "statusBar.noFolderBackground": "#0c2a45",
    "statusBarItem.remoteBackground": "#3D8D7A",
    "statusBarItem.remoteForeground": "#FEF9E1",
    
    // Title Bar
    "titleBar.activeBackground": "#0c2a45",
    "titleBar.activeForeground": "#E5D0AC",
    "titleBar.inactiveBackground": "#0c2a45",
    "titleBar.inactiveForeground": "#E5D0AC80",
    
    // Terminal
    "terminal.background": "#0c2a45",
    "terminal.foreground": "#E5D0AC",
    "terminal.ansiBlack": "#0c2a45",
    "terminal.ansiBlue": "#3674B5",
    "terminal.ansiGreen": "#3D8D7A",
    "terminal.ansiYellow": "#FF9D23",
    "terminal.ansiRed": "#C14600",
    "terminal.ansiMagenta": "#C14600",
    "terminal.ansiCyan": "#3D8D7A",
    "terminal.ansiWhite": "#FEF9E1",
    
    // Buttons & Inputs
    "button.background": "#3674B5",
    "button.foreground": "#FEF9E1",
    "button.hoverBackground": "#3D8D7A",
    "input.background": "#27445D",
    "input.foreground": "#E5D0AC",
    "input.border": "#3D8D7A40",
    
    // Tabs
    "tab.activeBackground": "#0c2a45",
    "tab.activeForeground": "#FF9D23",
    "tab.inactiveBackground": "#27445D",
    "tab.inactiveForeground": "#e5c998",
    "editorGroupHeader.tabsBackground": "#27445D"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#E5D0AC"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python"
      ],
      "settings": {
        "foreground": "#E5D0AC"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3D8D7A"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#3D8D7A"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "comment.block.documentation punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#3D8D7A"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#C14600"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#C14600"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#E5D0AC"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#FF9D23"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#3674B5"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#FF9D23"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#E5D0AC"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3674B5"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF9D23"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#f47b84"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#f0c677"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#90c17d"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#b6bcc8"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#7cb8f1"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#f0c677"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#b6bcc8"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#4a5261"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#7cb8f1"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7cb8f1"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f0c677"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#f0c677"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#f47b84"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#90c17d"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#7cb8f1"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#7cb8f1"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#f0c677"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#7cb8f1"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#90c17d"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#f0c677"
      }
    }
  ]
}
